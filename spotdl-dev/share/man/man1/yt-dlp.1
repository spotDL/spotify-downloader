$ yt-dlp --parse-metadata \[dq]video::(?P<formats>)\[dq] --write-info-json

# Replace all spaces and \[dq]_\[dq] in title and uploader with a \[ga]-\[ga]
$ yt-dlp --replace-in-metadata \[dq]title,uploader\[dq] \[dq][ _]\[dq] \[dq]-\[dq]
